class Dog:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def bark(self):
        return "Woof!"

    def birthday(self):
        self.age += 1
        return f"Happy {self.age}th Birthday, {self.name}!"






class Math:
    @staticmethod
    def add(x, y):
        return x + y






class Counter:
    count = 0

    @classmethod
    def increment(cls):
        cls.count += 1
        return cls.count






class Account:
    def __init__(self, owner, amount):
        self.owner = owner
        self.__balance = amount  # Private attribute

    def __update_balance(self, amount):  # Private method
        self.__balance += amount






